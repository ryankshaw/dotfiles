[user]
  name = Ryan Shaw
  email = ryankshaw@gmail.com
[gui]
  matchtrackingbranch = true
  pruneduringfetch = true
  fontui = -family Tahoma -size 13 -weight normal -slant roman -underline 0 -overstrike 0

[color]
  diff = auto
  status = auto
  branch = auto
[alias]
  c = commit -m
  a = add
  pl = pull
  ps = push
  plre = pull --rebase
  st = status

  thanks   = !git-thanks
  track    = !git-track
  ll       = log --pretty=oneline --abbrev-commit --max-count=15
  wip      = !git add -u && git commit -m WIP
  bv       = !git branch --color -v | cut -c1-100
  incoming = !git-incoming
  in       = !git-incoming
  outgoing = !git-outgoing
  out      = !git-outgoing
  up       = !git-up
  reup     = !git-reup
  s        = status -sb -uno --ignore-submodules=untracked
  st       = status
  sa       = status -sb --ignore-submodules=untracked

[core]
  excludesfile = /Users/ryan/.gitignore
  editor = mate -w
  whitespace = space-before-tab, trailing-space

[push]
  default = tracking
[github]
  user = ryankshaw
