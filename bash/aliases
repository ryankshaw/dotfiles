alias dream_proxy='echo "tell your browser to use localhost:8080 as a socks5 proxy,
as long as you keep this ssh window open you can use iamryanshaw.com as a proxy 
(command is: ssh iamryanshaw.com -D 8080)" & 
ssh iamryanshaw.com -D 8080'

